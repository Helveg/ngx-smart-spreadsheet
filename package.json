{
  "name": "ngx-spreadsheet",
  "version": "0.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-spreadsheet",
    "publish": "npm run build && npm publish dist/ngx-spreadsheet",
    "build:example": "ng build example",
    "watch": "ng build ngx-spreadsheet --watch --configuration development",
    "ghpages": "rimraf dist/example && ng build example --configuration production --base-href \"https://helveg.github.io/ngx-spreadsheet/\" && ngh --dir=dist/example",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.10",
    "@angular/common": "^16.2.10",
    "@angular/compiler": "^16.2.10",
